function myFunction1() {
    var div1 = document.getElementById("myDIV1");
    if (div1.style.display === "none") {
        div1.style.display = "block";
    } else {
        div1.style.display = "none";
    }
    var x;
    for (var i = 1; i <= 10; i++) {
        if (i == 1) {
            continue;
        } else {
            x = document.getElementById("myDIV"+i);
            x.style.display = "none";
        }
    }
}

